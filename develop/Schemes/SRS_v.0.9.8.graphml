      <data key="d5"><![CDATA[protected function addTarget(_ship:Ship, _noise:Number):void
		{
			var trg:Target;
			for each (trg in targets)
			{
				if (trg.ship == _ship)
				{
					trg.noise = _noise;
					return;
				}
			}
			trg = new Target(_ship, _noise);
			targets.push(trg);
		}]]></data>
