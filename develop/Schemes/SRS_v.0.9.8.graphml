			//-- для того который под ручным управление не выбираем цель
			if (under_control)
			{
				return null;
			}
			////-- пока исходим из того, что я один ))
			if (forces == Constants.FORCES_RED)
			{
				for each (var s:Ship in Main.main.getRedShips())
				{
					if (s.isConvoy())
					{
						return s;
					}
				}
